<h1>Show booking</h1>
<div class="container-beaches row">
    <p>Booking Reference: <%= @booking.id %></p>
    <p>Beach: <%= @booking.beach.name %></p>
    <p>Start Date</p>
    <%= @booking.start_date %>
    <p>End Date</p>
    <%= @booking.end_date %>
     <%= link_to "Cancel", beach_booking_path(@booking), class: "btn btn-primary ms-3 me-3", data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
</div>
